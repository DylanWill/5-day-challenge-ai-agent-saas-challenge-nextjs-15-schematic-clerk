# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key

# Schematic
NEXT_PUBLIC_SCHEMATIC_PUBLISHABLE_KEY=api_your_schematic_publishable_key
SCHEMATIC_API_KEY=sch_dev_your_schematic_api_key

# Youtube API
YOUTUBE_API_KEY=your_youtube_api_key

# Deployment used by `npx convex dev`
CONVEX_DEPLOYMENT=dev:your-convex-deployment # team: your-team, project: your-project

NEXT_PUBLIC_CONVEX_URL=https://your-convex-deployment.convex.cloud

# Claude API
CLAUDE_API_KEY=sk-ant-api03-your_claude_api_key

# OpenAI API
OPENAI_API_KEY=sk-your_openai_api_key

# Redfin API (for market data)
REDFIN_API_KEY=your_redfin_api_key

# Zillow API (for market data via Rapid API)
ZILLOW_API_KEY=

# ATTOM API (for property data and analytics)
ATTOM_API_KEY=your_attom_api_key

# QuickChart API (for visualizations)
QUICKCHART_API_KEY=your_quickchart_api_key

# Google API (for Gmail, Drive, Calendar, and Contacts)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google

# Encryption (for secure token storage)
# Generate a secure key with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your_32_character_encryption_key

# Email Provider API (MailerSend)
MAILERSEND_API_KEY=your_mailersend_api_key
MAILERSEND_FROM_EMAIL=your_verified_sender@example.com
MAILERSEND_FROM_NAME=Your Name or Company
